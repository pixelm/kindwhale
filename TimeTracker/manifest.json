{
   "manifest_version": 2,

   "name": "Time Tracker",
   "description": "Time Tracker",
   "version": "0.1",
   "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html",
      "default_title": "Time Tracker"
   },
   "icons": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
   },
   "content_scripts": [
      {
         "matches": ["<all_urls>"],
         "css": ["css/cs_styles.css"],
         "js": ["libs/jquery-1.12.0.min.js", "scripts/actions.js", "scripts/tools.js", "scripts/cs_bootstrap.js"],
         "run_at": "document_end"
    }
  ],
   "background": {
      "scripts": ["libs/jquery-1.12.0.min.js", "libs/moment.js", "scripts/actions.js", "scripts/tools.js", "scripts/bs_main.js"]
   },
   "permissions": [
      "activeTab",
      "tabs",
      "<all_urls>",
      "https://*/*",
      "http://*/*"
   ],
   "web_accessible_resources": ["css/cs_styles.css"]
}